{
  "overwrite": true,
  "config": {
    "strict": true,
    "namingConvention": {
      "typeNames": "pascal-case#pascalCase",
      "enumValues": "keep"
    },
    "scalars": {
      "ID": "string | number",
      "Money": "number"
    },
    "maybeValue": "T"
  },
  "generates": {
    "src/ui/generated-types.ts": {
      "schema": "http://localhost:3050/admin-api",
      "documents": "src/ui/**/*.graphql.ts",
      "plugins": ["typescript", "typescript-operations", "typed-document-node"],
      "config": {
        "scalars": {
          "ID": "string"
        }
      }
    }
  }
}
